<div class="modal" id="{{userid}}" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">{{tabledata.modulename}}</h4>
            </div>
            <div class="modal-body">
                <div class="position"><p>Time Writing:{{tabledata.total_time_seconds}} </p><span></span></div>
                <div class="position"><p>Words per minute: {{tabledata.words_per_minute}} </p><span></span></div>
                <div class="position"><p>Total Words:{{tabledata.word_count}} </p><span></span></div>
                <div class="position"><p>Backspace %: {{tabledata.backspace_percent}}</p><span></span></div>
            </div>
            <div class="modal-footer">
                <button type="button" id="modal-close{{userid}}" data-resource="{{userid}}"
                        class="btn btn-primary" data-dismiss="modal">Close
                </button>
            </div>
        </div>
    </div>
</div>
<div class="modal" id="typeid{{userid}}" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">{{tabledata.modulename}}</h4>
            </div>
            <div class="modal-footer">
                <button type="button" id="modal-close{{userid}}" data-resource="{{userid}}" class="btn btn-primary"
                        data-dismiss="modal">Close
                </button>
            </div>
        </div>
    </div>
</div>
<div class="modal" id="playback{{userid}}" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Playback Video</h4>
            </div>
            <div class="modal-body">
                <div id="output_playback_{{userid}}"></div>
            </div>
            <div class="modal-footer">
                <button type="button" id="modal-close-playback{{userid}}" data-resource="{{userid}}"
                        class="btn btn-primary" data-dismiss="modal">Close
                </button>
            </div>
        </div>
    </div>
</div>

